spring.application.name=JavaFrameworkGeneralFunction
spring.datasource.url=jdbc:mysql://localhost:3306/general_function
spring.datasource.username=generalfunction
spring.datasource.password=generalfunction
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver

logging.file.name=logs/app.log
logging.file.path=logs
logging.pattern.file=%d{yyyy-MM-dd HH:mm:ss} %-5level %logger{36} - %msg%n
logging.level.root=INFO

